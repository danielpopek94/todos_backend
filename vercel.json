{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/todos/(.*)",
      "dest": "./app.js",
      "methods": ["GET", "POST", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}